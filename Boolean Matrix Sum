      Scanner sc=new Scanner(System.in);
      int m=sc.nextInt();
      int n=sc.nextInt();
      int [][]mat=new int[m][n];
      for(int i=0;i<m;i++){
        int flag=0;
      
      for(int j=0;j<n;j++){
        mat[i][j]=sc.nextInt();
        if(mat[i][j]==1){
          flag=1;
        }
      }
      for(int j=0;j<n;j++){
        System.out.print(flag+ " ");
      }
       System.out.println();
	}
